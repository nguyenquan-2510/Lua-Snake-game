{
    "Lua.diagnostics.disable": [
        "unused-local",
        "lowercase-global"
    ],
    "editor.fontSize": 25,
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "Cascadia code",
    "code-runner.executorMap": {
        "lua": "love .",
    },
    "code-runner.clearPreviousOutput": true,
